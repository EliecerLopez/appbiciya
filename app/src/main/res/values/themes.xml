<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Appbiciya" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="titulo">
        <item name="android:textColor">@color/colortitulo</item>
        <item name="android:textSize">40dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:fontFamily">Arsenal</item>
    </style>
    <style name="txtbot">
        <item name="android:textColor">@color/colorbot</item>
        <item name="android:textSize">40dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:fontFamily">Arsenal</item>
        <item name="android:background">@color/purple_700</item>

    </style>



</resources>